    <table class="table table-striped table-hover table-sm table-bordered">
        <caption style="caption-side: top;">public keys: {{ public_keys.length }}</caption>
        <colgroup>
            <col width="14%">
            <col width="14%">
            <col width="1%">
            <col width="22%">
            <col>
        </colgroup>
        <thead>
        <tr>
            <th>first&nbsp;seen</th>
            <th>last&nbsp;seen</th>
            <th>bits</th>
            <th>key&nbsp;fingerprint</th>
            <th>key&nbsp;name</th>
        </tr>
        </thead>
        <tbody>
        {{#each public_keys}}
        <tr>
            <td><nobr>{{ this.created_at }}</nobr></td>
            <td><nobr>{{ this.updated_at }}</nobr></td>
            <td>{{ this.bits }}</td>
            <td>{{ this.fingerprint }}</td>
            <td>{{ this.comment }}</td>
        </tr>
        {{/each}}
        </tbody>
    </table>